package com.gio.inheritance;

import javax.persistence.DiscriminatorValue;
import javax.persistence.Entity;

@Entity
@DiscriminatorValue(value="C")
public class Car extends Vehicle {

	private int speed;
	
	public Car() {}

	public Car(String name, int speed) {
		super(name);
		this.speed = speed;
	}
	
	
	
}
