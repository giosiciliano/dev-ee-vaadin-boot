package com.gio;

public class Student {

	private String name;

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}
	
	public void studentInit() {
		System.out.println("studentInit method called");
	}
	
	public void studentDestroy() {
		System.out.println("studentDestroy method called");
	}

}
